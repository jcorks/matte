    switch(a->binID) {

      case MATTE_VALUE_TYPE_NUMBER: 
        matte_value_into_number(
            a, 
            matte_value_as_number(*a) *
            matte_value_as_number(b)
        );
        break;
      
      case MATTE_VALUE_TYPE_OBJECT: {
        result = vm_run_object_operator_2(vm, *a, "**=", b);
        break;
      }      

      default: 
        bad_operator(vm, "**=", *a);
        return result;

    }
    matte_value_into_copy(&result, *a);
    return result;
}

